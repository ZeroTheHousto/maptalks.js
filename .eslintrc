{
  "root": true,
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "standard",
  "globals": {
  },
  "rules": {
    "no-var" : "error",
    "prefer-const": "error",
    "global-require": 0,
    "no-prototype-builtins": "warn",
    "no-undef": "warn",
    "new-cap": "warn",
    "no-mixed-operators": "warn",
    "no-useless-constructor": "warn"
  }
}
